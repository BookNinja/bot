{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src",
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["*"],
      "@controllers": ["./src/controllers"],
      "@routes/*": ["./src/routes/*"]
    }
  },
  "include": ["src/**/*"]
}
